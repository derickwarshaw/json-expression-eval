  "bugs": {
    "url": "https://github.com/regevbr/json-expression-eval/issues"
  },
  "homepage": "https://github.com/regevbr/json-expression-eval#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "@types/underscore": "^1.8.8",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "ts-node": "^6.1.0",
    "typescript": "^2.9.1"
  },

{
  "name": "json-expression-eval",
  "version": "0.0.2",
  "description": "evaluate a json described boolean expression using dynamic functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha test/ --reporter spec --compilers ts:ts-node/register --recursive",
    "build": "npm run buildDist && npm run buildExample",
    "buildDist": "./node_modules/.bin/tsc",
    "buildExample": "cd ./example && tsc",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha test/ -- -R spec --compilers ts:ts-node/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regevbr/json-expression-eval.git"
  },
  "keywords": [
    "evaluate",
